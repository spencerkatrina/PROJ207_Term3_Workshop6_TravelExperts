<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <ComboBox fx:id="cbPackages" layoutX="231.0" layoutY="20.0" prefWidth="150.0" />
      <VBox layoutX="14.0" layoutY="68.0" prefHeight="195.0" prefWidth="206.0">
         <children>
            <Label text="Package ID" />
            <Label text="Agency Commission" />
            <Label text="Package Base Price" />
            <Label text="Package Description" />
            <Label text="Package End Date" />
            <Label text="Package Start Date" />
            <Label text="Package Name" />
         </children>
      </VBox>
      <VBox layoutX="236.0" layoutY="66.0" prefHeight="242.0" prefWidth="181.0">
         <children>
            <TextField fx:id="tfPackageId" />
            <TextField fx:id="tfAgencyCommission" />
            <TextField fx:id="tfBasePrice" />
            <TextField fx:id="tfDescription" />
            <TextField fx:id="tfEndDate" />
            <TextField fx:id="tfStartDate" />
            <TextField fx:id="tfPackageName" />
         </children>
      </VBox>
      <HBox layoutX="74.0" layoutY="297.0" prefHeight="100.0" prefWidth="414.0">
         <children>
            <Button fx:id="btnPackageEdit" mnemonicParsing="false" text="Edit" />
            <Button fx:id="btnPackageSave" mnemonicParsing="false" text="Save" />
            <Button fx:id="btnPackageDelete" mnemonicParsing="false" text="Delete" />
            <Button fx:id="btnPackageClose" mnemonicParsing="false" text="Close" />
         </children>
      </HBox>
   </children>
</AnchorPane>
